etOpenWindow(OpenWindows.AUTH)\r\n  }\r\n\r\n  const recoveryClickHandler = async (evt) => {\r\n    evt.preventDefault()\r\n    try {\r\n      const data = await request('/api/auth/recovery', 'POST', { ...form })\r\n      console.log(`\"Новый пароль отправлен на почту \" ${data}`)\r\n    } catch (e) {\r\n      setError(setTypeError(e.message))\r\n    }\r\n  }\r\n\r\n  const onBlurEmailHandler = (evt) => {\r\n    !evt.target.value && setFocusStyleEmail(false)\r\n    let reg = /^([A-Za-z0-9_\\-\\.])+\\@([A-Za-z0-9_\\-\\.])+\\.([A-Za-z]{2,4})$/;\r\n    const emailValid = reg.test(evt.target.value)\r\n    setValidEmail(emailValid)\r\n    emailValid ? setError({}) : setError({type:\"email\", message:\"Введите корректный Email\"})\r\n  }\r\n\r\n  const changeStyleOnFocusEmail = () => {\r\n    setFocusStyleEmail(true)\r\n  }\r\n\r\n  return (\r\n    <div className={\"password-recovery \" + ((openWindow===\"Recovery\") && \"password-recovery--open\")}>\r\n      <h3 className=\"password-recovery__title login__form-title\">Восстановление пароля</h3>\r\n      <div className=\"login__input-form input-form__recovery\">\r\n      <input className={((!validEmail || (error.type === \"email\")) ? \"error-validation__input \" : \"\") + \"login__input \" + (focusStyleEmail && \"login__input--focus\")}\r\n            id=\"input-login\"\r\n            placeholder=\"e-mail@mail.ru\"\r\n            type=\"text\"\r\n            name=\"email\"\r\n            onChange={changeHandler}\r\n            onFocus={changeStyleOnFocusEmail}\r\n            onBlur={onBlurEmailHandler}\r\n          />\r\n        <label className=\"login__label\" htmlFor=\"input-login\">E-mail*</label>\r\n        {error.type === \"email\" && <span className=\"error-validation__message\">{error.message}</span>}\r\n\r\n      </div>\r\n      <div className=\"warning\">\r\n        <div className=\"exclamation-mark\"></div>\r\n        <p className=\"warning__text\">Пароль будет отправлено на электронную почту, к которой привязана учетная запись.</p>\r\n      </div>\r\n      <div className=\"line\"></div>\r\n      <div className=\"login__choice\">\r\n        <button className=\"choice__back choice__btn choice__btn-first\"\r\n          onClick={clickBackHandler}\r\n        >Назад</button>\r\n        <button className=\"choice__recovery choice__btn choice__btn-second\"\r\n        type=\"submit\"\r\n        onClick={recoveryClickHandler}\r\n        >Восстановить</button>\r\n      </div>\r\n    </div>\r\n  )\r\n}","C:\\Users\\Ilya\\Desktop\\Proj\\Venta-mebel\\serverMongoDb\\AuthWithMongoDB\\my-app\\src\\hooks\\http.hook.js",[],"C:\\Users\\Ilya\\Desktop\\Proj\\Venta-mebel\\serverMongoDb\\AuthWithMongoDB\\my-app\\src\\utlis.js",[],"C:\\Users\\Ilya\\Desktop\\Proj\\Venta-mebel\\serverMongoDb\\AuthWithMongoDB\\my-app\\src\\const.js",[],{"ruleId":"44","replacedBy":"45"},{"ruleId":"46","replacedBy":"47"},{"ruleId":"48","severity":1,"message":"49","line":74,"column":31,"nodeType":"50","messageId":"51","endLine":74,"endColumn":32,"suggestions":"52"},{"ruleId":"48","severity":1,"message":"53","line":74,"column":36,"nodeType":"50","messageId":"51","endLine":74,"endColumn":37,"suggestions":"54"},{"ruleId":"48","severity":1,"message":"49","line":74,"column":52,"nodeType":"50","messageId":"51","endLine":74,"endColumn":53,"suggestions":"55"},{"ruleId":"48","severity":1,"message":"49","line":38,"column":31,"nodeType":"50","messageId":"51","endLine":38,"endColumn":32,"suggestions":"56"},{"ruleId":"48","severity":1,"message":"53","line":38,"column":36,"nodeType":"50","messageId":"51","endLine":38,"endColumn":37,"suggestions":"57"},{"ruleId":"48","severity":1,"message":"49","line":38,"column":52,"nodeType":"50","messageId":"51","endLine":38,"endColumn":53,"suggestions":"58"},"no-native-reassign",["59"],"no-negated-in-lhs",["60"],"no-useless-escape","Unnecessary escape character: \\..","Literal","unnecessaryEscape",["61","62"],"Unnecessary escape character: \\@.",["63","64"],["65","66"],["67","68"],["69","70"],[